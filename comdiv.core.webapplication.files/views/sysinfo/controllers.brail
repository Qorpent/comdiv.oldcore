#pragma boo
import Castle.MonoRail.Framework
defines :
	result as (IController)
bml :
	foreach i=result :
		prepare :
			fullname = (i.GetType() as System.Type).FullName
			aname = (i.GetType() as System.Type).Assembly.GetName().Name
		astable "sysinfo controllers" :
			heads  "Тип", "Сборка"
			cells fullname, aname 